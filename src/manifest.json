{
	"name": "Gmail Color Diff",
	"version": "0.0.1",
	"manifest_version": 2,

	"description": "This extension colors diff patches inline a Gmail email.",

	"permissions": [
		"https://mail.google.com/*"
	],

	"content_scripts": [{
		"matches": ["https://mail.google.com/*"],
		"js": ["gcolordiff.js"],
		"run-at": "document_end"
	}]
}
